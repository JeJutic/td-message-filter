<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="392.0" prefWidth="476.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.jejutic.tdmessagefilter.ui.controller.TagListController">
    <ListView fx:id="tags" editable="true" layoutY="46.0" prefHeight="310.0" prefWidth="476.0" />
    <Button fx:id="removeTagButton" layoutX="327.0" layoutY="360.0" mnemonicParsing="false" onAction="#onRemoveTagClick" text="Remove selected tag" />
   <TextField fx:id="newTagField" layoutX="14.0" layoutY="14.0" prefHeight="26.0" prefWidth="342.0" promptText="Enter new tag here" />
   <Button fx:id="addNewTagButton" layoutX="362.0" layoutY="14.0" mnemonicParsing="false" onAction="#onAddNewTagClick" prefHeight="26.0" prefWidth="97.0" text="Add tag" />
</AnchorPane>
